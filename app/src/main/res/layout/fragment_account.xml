<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.brim.FotterFragmnet.Account">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="10"
        >

        <RelativeLayout
            android:id="@+id/button_pro"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/p_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/icon_account_navy"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/p_icon"
                android:layout_marginLeft="25dp"
                android:text="Profile"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />

        <RelativeLayout
            android:id="@+id/button_mang_card"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/crd_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/icon_card"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/crd_icon"
                android:layout_marginLeft="25dp"
                android:text="Manage Card"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />

        <RelativeLayout
            android:id="@+id/button_settings"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/settings_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/icon_settings"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/settings_icon"
                android:layout_marginLeft="25dp"
                android:text="Settings and Notifications"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />

        <RelativeLayout
            android:id="@+id/button_travel"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/travel_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/icon_travel"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/travel_icon"
                android:layout_marginLeft="25dp"
                android:text="Travel Notice"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />


        <RelativeLayout
            android:id="@+id/button_help"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/help_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/icon_help"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/help_icon"
                android:layout_marginLeft="25dp"
                android:text="Help"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />

        <RelativeLayout
            android:id="@+id/button_legal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/legal_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/icon_legalprivacy"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/legal_icon"
                android:layout_marginLeft="25dp"
                android:text="Legal and Privacy"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />

        <RelativeLayout
            android:id="@+id/button_signin_m"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/signin_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/pin_icon"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/signin_icon"
                android:layout_marginLeft="25dp"
                android:text="Sign-in Method"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />

        <RelativeLayout
            android:id="@+id/button_logout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingLeft="30dp"
            android:background="@color/white"
            android:gravity="center_vertical"
            >

            <ImageView
                android:id="@+id/logout_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/icon_logout"
                />

            <com.brim.Font.SFNFTextView
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_toRightOf="@+id/logout_icon"
                android:layout_marginLeft="25dp"
                android:text="Log Out"
                android:gravity="center"
                android:textSize="14sp"
                android:textColor="@color/textmediamGray"
                />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/BorderGray"
            />



    </LinearLayout>



</FrameLayout>
